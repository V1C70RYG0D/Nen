{
  "name": "nen-backend",
  "version": "0.1.0",
  "description": "Nen Platform Enhanced POC Backend - Advanced Express.js API with caching, security, and performance optimization",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "db:seed": "npx tsx prisma/seed.ts",
    "start:production": "NODE_ENV=production node dist/main.js",
    "start:test": "NODE_ENV=test PORT=3001 node dist/main.js",
    "dev": "node src/temp-server.js",
    "dev:optimized": "tsx watch src/server-optimized.ts",
    "test": "jest --coverage",
    "test:optimized": "jest --testPathPattern=optimized --verbose",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:load": "jest --testNamePattern=load",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:integration": "jest --testPathPattern=integration --coverage",
    "test:websocket": "jest --config=jest.websocket.config.js --verbose",
    "test:websocket:connection": "jest --config=jest.websocket.config.js --testPathPattern=connection-tests --verbose",
    "test:websocket:load": "jest --config=jest.websocket.config.js --testPathPattern=load-tests --verbose",
    "test:websocket:security": "jest --config=jest.websocket.config.js --testPathPattern=security-tests --verbose",
    "test:websocket:coverage": "jest --config=jest.websocket.config.js --coverage --verbose",
    "test:websocket:watch": "jest --config=jest.websocket.config.js --watch --verbose",
    "test:services": "jest --testPathPattern=services --coverage",
    "test:security": "jest --testPathPattern=security --coverage",
    "test:performance": "jest --testNamePattern=performance --verbose",
    "test:all": "jest --coverage --verbose",
    "test:cov": "jest --coverage --ci --watchAll=false --maxWorkers=2",
    "test:ci": "jest --coverage --ci --watchAll=false --maxWorkers=2",
    "test:setup": "bash scripts/start-test-services.sh",
    "test:teardown": "bash scripts/stop-test-services.sh",
    "test:clean": "bash scripts/stop-test-services.sh --clean",
    "test:infrastructure": "npm run test:setup && npm run test && npm run test:teardown",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit",
    "performance:profile": "clinic doctor -- node dist/server-optimized.js",
    "docker:build": "docker build -t nen-backend .",
    "docker:run": "docker run -p 3001:3001 nen-backend",
    "launch:optimized": "bash launch-optimized.sh"
  },
  "keywords": [
    "blockchain",
    "gaming",
    "solana",
    "gungi",
    "api",
    "performance",
    "security",
    "caching"
  ],
  "author": "Nen Platform Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@solana/web3.js": "^1.98.2",
    "@types/compression": "^1.8.1",
    "@types/pg": "^8.15.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bs58": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "prisma": "^6.0.1",
    "prom-client": "^15.1.3",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@mswjs/data": "^0.16.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/bs58": "^4.0.4",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "artillery": "^2.0.21",
    "c8": "^10.1.3",
    "cross-env": "^10.0.0",
    "eslint": "^9.16.0",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-each": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "jest-extended": "^6.0.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^4.0.0",
    "jsdom": "^26.1.0",
    "k6": "^0.0.0",
    "msw": "^2.10.4",
    "nock": "^14.0.8",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.4"
  },
  "engines": {
